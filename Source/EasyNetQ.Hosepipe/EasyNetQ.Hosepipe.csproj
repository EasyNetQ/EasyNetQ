<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>EasyNetQ.Hosepipe</Description>
    <TargetFramework>net6</TargetFramework>
    <OutputType>Exe</OutputType>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Fody" Version="6.6.3">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="NullGuard.Fody" Version="3.1.0" PrivateAssets="All" />
    <PackageReference Include="RabbitMQ.Client" Version="[6.2.1,6.5.0)" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\EasyNetQ\EasyNetQ.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Content Remove="Usage.txt" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Usage.txt" />
  </ItemGroup>
</Project>
